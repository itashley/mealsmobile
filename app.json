{
  "expo": {
    "name": "Ashley Meals",
    "slug": "mealsapp",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icons/meal.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/mealscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFDD0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "mealsIcon": {
        "foregroundImage": "./assets/icons/meal.png",
        "backgroundColor": "#FFFDD0"
      },
      "package": "com.ashley.mealsapp"
    },
    "web": {
      "favicon": "./assets/icons/meal.png",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no"
      },
      "template": "./expo/web/index.html"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "1a628cd1-c5d8-4219-9a54-74e267204443"
      }
    }
  }
}
